# Ignore node_modules directory in both client and server folders
chat-app-client/node_modules/
chat-app-server/node_modules/

# Ignore .env files containing sensitive information
chat-app-client/.env
chat-app-server/.env

# Ignore build output and other generated files
chat-app-client/build/
chat-app-server/build/
chat-app-client/dist/
chat-app-server/dist/

# Ignore log files
*.log
logs/

